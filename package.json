{
  "name": "fileorganizer",
  "productName": "The File Organizer",
  "version": "1.0.0",
  "description": "A simple app that organizes your messy files",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-packager . fileorganizer --overwrite --platform win32 --arch x64 --out tmp/dist/ --icon assets/icons/win/icon.ico",
    "setup": "electron-installer-windows --src tmp/dist/fileorganizer-win32-x64/ --dest tmp/dist/installers --icon assets/icons/win/icon.ico --animation assets/img/installing.gif"
  },
  "author": "Oliwier Gesla",
  "license": "MIT",
  "devDependencies": {
    "electron-installer-windows": "^2.0.0",
    "electron-packager": "^14.0.4"
  },
  "dependencies": {
    "electron": "^1.8.4",
    "electron-settings": "^3.2.0",
    "electron-squirrel-startup": "^1.0.0",
    "jquery": "^3.4.1",
    "python-shell": "^1.0.7"
  }
}
